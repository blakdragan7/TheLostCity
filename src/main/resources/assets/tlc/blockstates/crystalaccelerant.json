{
    "multipart": [
        {
            "when": {"is_activated":"true"},
            "apply": { "model": "redstone_dot" }
        },
        {   "when": { "OR": [
                {"north": "none", "east": "none", "south": "none", "west": "none"},
                {"north": "side|up", "east": "side|up",  "is_activated":"false" },
                {"east": "side|up", "south": "side|up",  "is_activated":"false" },
                {"south": "side|up", "west": "side|up",  "is_activated":"false"},
                {"west": "side|up", "north": "side|up",  "is_activated":"false"}
            ]},
            "apply": { "model": "redstone_dot" }
        },
        {   "when": { "OR": [
                { "north": "side|up",  "is_activated":"false" },
                { "north": "none", "east": "none", "south": "side|up", "west": "none",  "is_activated":"false" }
            ]},
            "apply": { "model": "redstone_side0" }
        },
        {   "when": { "OR": [
                { "south": "side|up",  "is_activated":"false" },
                { "north": "side|up", "east": "none", "south": "none", "west": "none",  "is_activated":"false" }
            ]},
            "apply": { "model": "redstone_side_alt0" }
        },
        {   "when": { "OR": [
                { "east": "side|up",  "is_activated":"false" },
                { "north": "none", "east": "none", "south": "none", "west": "side|up",  "is_activated":"false" }
            ]},
            "apply": { "model": "redstone_side_alt1", "y": 270 }
        },
        {   "when": { "OR": [
                { "west": "side|up",  "is_activated":"false" },
                { "north": "none", "east": "side|up", "south": "none", "west": "none",  "is_activated":"false" }
            ]},
            "apply": { "model": "redstone_side1", "y": 270 }
        },
        {   "when": { "north": "up",  "is_activated":"false" },
            "apply": { "model": "redstone_up" }
        },
        {   "when": { "east": "up",  "is_activated":"false" },
            "apply": { "model": "redstone_up", "y": 90 }
        },
        {   "when": { "south": "up",  "is_activated":"false" },
            "apply": { "model": "redstone_up", "y": 180 }
        },
        {   "when": { "west": "up",  "is_activated":"false" },
            "apply": { "model": "redstone_up", "y": 270 }
        }
    ]
}